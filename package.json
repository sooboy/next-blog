	"main": "index.js",
	"scripts": {
		"clear": "rm -rf _next app app.tar app_static.tar",
		"zip": "tar zcf app.tar --exclude=node_modules *",
		"zip_static": "tar zcf app_static.tar static _next/static",
		"build": "NODE_ENV=production next build",
		"dev": "nodemon",
		"exec": "NODE_ENV=production ts-node -r tsconfig-paths/register --project tsconfig.server.json server/index.ts",
		"production": "npm run clear && npm run build && npm run zip && npm run zip_static ",
		"analyze_server": "BUNDLE_ANALYZE=server yarn build",
		"analyze_browser": "BUNDLE_ANALYZE=browser yarn build",
		"analyze_both": "BUNDLE_ANALYZE=both yarn build"
	},
